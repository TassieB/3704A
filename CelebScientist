  <body>
    <script>
        const celscilist = {
          'yn': { 'info': { 'views': 440 } },
        };
        
        class Celsci {
          static iscelsci(x) {
            let y = "false";
            if (x > 500) {
              y = "true";
            }
            return y;
          }
        }
        
        function checkcelsci(x) {
          const result = Celsci.iscelsci(celscilist[x]['info']['views']);
          return result;
        }
        
        document.write(checkcelsci('yn'));     
    </script>
  </body>
